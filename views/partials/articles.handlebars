<div class="col s12 m6 l4 left">
  <div class="card" id="articles">
    <div class="card-image">
      <img src="{{image}}">
      <span class="card-title"></span>
    </div>
    <div class="card-content">
      <p class="title">{{title}}</p>
    </div>
    <div class="card-action">
      <a href="{{link}}">Read Article</a>
    </div>
  </div>
</div>
